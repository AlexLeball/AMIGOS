<div id="categories">

  <% categories = @categories.sort_by{ |m| m.name } %>

  <% categories.each do |category| %>
    <%= link_to category_path(category) do %>
      <%= cl_image_tag category.photo.key, height: 500, width: 500, crop: :fill, class:"category-image" %>
      <p><%= category.name.capitalize %></p>
    <% end %>
  <% end %>
</div>
