<%= simple_form_for [user] do |f| %>
  <%= f.input :first_name, label: "Prénom" %>
  <%= f.input :last_name, label: "Nom" %>
  <%= f.input :birth_date, label: "Date de naissance", as: :date, html5: true %>
  <%= f.input :city, label: "Ville" %>
  <%= f.input :photo, label: "Photo", as: :file %>
  <%= f.button :submit, "Mettre à jour", class: "button-primary-register-no-margin a-style" %>
<% end %>
